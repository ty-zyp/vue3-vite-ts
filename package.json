    "@rushstack/eslint-patch": "^1.1.4",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.19",
    "@unocss/preset-attributify": "^0.49.4",
    "@unocss/preset-icons": "^0.49.4",
